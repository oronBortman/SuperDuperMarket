<FlowPane prefHeight="158.0" prefWidth="128.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.addingDiscounts.Item.QuantityItem.QuantityItemController">
   <children>
      <Pane prefHeight="190.0" prefWidth="181.0" style="-fx-border-color: gray; -fx-border-width: 5;">
         <children>
            <Label fx:id="itemNameLabel" layoutX="44.0" layoutY="37.0" text="Item name">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="labelForAdditional" layoutX="13.0" layoutY="85.0" text="For Additional" />
